  },
  "main": "server.js",
  "author": "Brendan Irvine-Broque <brendanib@gmail.com>",
  "scripts": {
    "dev": "NODE_ENV=development node_modules/.bin/supervisor --ignore build/ -e js server & NODE_ENV=development node hotLoadServer.js --progress",
