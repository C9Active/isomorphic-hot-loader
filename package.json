    "flux": "^2.0.1",
    "fuzzy": "^0.1.0",
    "hammerjs": "^2.0.4",
    "moment": "^2.8.4",
    "node-jsx": "^0.12.0",
    "object-assign": "^2.0.0",
    "react": "^0.12.0",
    "react-document-title": "^0.1.3",
    "react-hammerjs": "^0.1.3",
    "react-router": "^0.11.6",
    "rest": "^1.2.0",
    "when": "^3.6.3"
  },
  "devDependencies": {
    "eslint": "^0.11.0-alpha.0",
